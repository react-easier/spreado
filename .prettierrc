{
  "arrowParens": "always",
  "bracketSameLine": false,
  "bracketSpacing": false,
  "printWidth": 100,
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "es5",
  "useTabs": false,
  "importOrder": ["<THIRD_PARTY_MODULES>", "^\\."],
  "importOrderCaseInsensitive": true,
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
